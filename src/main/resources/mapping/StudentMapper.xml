<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.accp.springmvc_2.dao.StudentMapper" >
 	<select id="query" resultType="com.accp.springmvc_2.pojo.Student">
 		select * from student
 	</select>
 	<select id="querybyname" resultType="com.accp.springmvc_2.pojo.Student">
 		select * from student where stuname=#{name}
 	</select>
</mapper>